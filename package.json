{
  "name": "@ksz13/gh-actions-basic",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.24.0",
  "description": "Basic GitHub Actions workflow for a TypeScript package.",
  "author": "Kensong Zhu <kensongzhu@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kensongzhu/gh-actions-basic#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensongzhu/gh-actions-basic.git"
  },
  "bugs": {
    "url": "https://github.com/kensongzhu/gh-actions-basic/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "bumpp": "^10.3.2",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
